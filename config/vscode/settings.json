{
    "vim.handleKeys": {
        "<C-p>": false,
        "<C-w>": false
    },
    "vim.normalModeKeyBindings": [
        {
            "before": ["<leader>", "z"],
            "commands": ["workbench.action.toggleZenMode"]
          },
          {
            "before": ["<leader>", "s", "t"],
            "commands": ["workbench.action.toggleSidebarVisibility"]
          },
          {
            "before": ["<leader>", "s", "x"],
           "commands": ["workbench.view.extensions"]
          },
          {
            "before": ["<leader>", "s", "e"],
            "commands": ["workbench.view.explorer"]
          },
          {
            "before": ["<leader>", "s", "f"],
            "commands": ["workbench.view.search"]
          },
          {
            "before": ["<leader>", "s", "s"],
            "commands": ["workbench.view.scm"]
          },
          {
            "before": ["<leader>", "s", "g"],
            "commands": ["workbench.panel.chat.view.copilot.focus"]
          },
          {
            "before": ["<leader>", "f", "f"],
            "commands": ["workbench.action.files.openFolder"]
          },
          {
            "before": ["<leader>", "f", "o"],
            "commands": ["workbench.action.files.openFile"]
          },
          {
            "before": ["<leader>", "c", "f"],
            "commands": ["editor.fold"],
            "when": "editorTextFocus"
          },
          {
            "before": ["<leader>", "c", "u"],
            "commands": ["editor.unfold"],
            "when": "editorTextFocus"
          },
          {
            "before": ["<leader>", "g", "w"],
            "commands": ["workbench.action.closeEditorsInGroup"]
          },
          {
            "before": ["<leader>", "g", "<Left>"],
            "commands": ["workbench.action.moveEditorToPreviousGroup"]
          },
          {
            "before": ["<leader>", "g", "<Right>"],
            "commands": ["workbench.action.moveEditorToNextGroup"]
          },
          {
            "before": ["<leader>", "f"],
            "after": ["<C-f>"]
          },
          {
            "before": ["<leader>", "b"],
            "after": ["<C-b>"]
          },
          {
            "before": ["<leader>", "i"],
            "after": ["<C-i>"]
          },
          {
            "before": ["<leader>", "r"],
            "after": ["<C-r>"]
          },
          {
            "before": ["<leader>", "w"],
            "commands": ["workbench.action.closeActiveEditor"],
            "when": "editorTextFocus"
          },
          {
            "before": ["<leader>", "h"],
            "commands": ["workbench.action.navigateLeft"],
            "when": "editorTextFocus"
          },
          {
            "before": ["<leader>", "j"],
            "commands": ["workbench.action.navigateDown"],
            "when": "editorTextFocus"
          },
          {
            "before": ["<leader>", "l"],
            "commands": ["workbench.action.navigateRight"],
            "when": "editorTextFocus"
          },
          {
            "before": ["<leader>", "/"],
            "commands": ["editor.action.commentLine"],
            "when": "editorTextFocus"
          },
          {
            "before": ["<leader>", "\\"],
            "commands": ["workbench.action.splitEditorRight"],
            "when": "editorTextFocus"
          }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["<leader>", "/"],
            "commands": ["editor.action.commentLine"],
            "when": "editorTextFocus"
        }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ],
    "vim.commandLineModeKeyBindings": [],
    "vim.commandLineModeKeyBindingsNonRecursive": []
}
